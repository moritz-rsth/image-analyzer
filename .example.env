REPLICATE_API_TOKEN=yourToken
SECRET_KEY=your-secret-key-here
ADMIN_PASSWORD_HASH=your-sha256-hash-here

# APP_BASE_URL: Auto-configured on Railway, required on GCP, optional for local dev
# Railway: Auto-set from RAILWAY_PUBLIC_DOMAIN (don't set manually)
# GCP: Set to your VM's external IP (e.g., http://34.123.45.67:5000)
# Local: Optional, defaults to http://localhost:5000
# APP_BASE_URL=http://localhost:5000

PORT=5000
FLASK_ENV=development # leave blank if deployed